{
  "name": "i-secure-send-api",
  "version": "1.0.0",
  "description": "end-to-end file sharing system",
  "main": "app.js",
  "scripts": {
    "start": "babel-node build/app.js",
    "dev-start": "nodemon --exec babel-node src/app.js",
    "build": "./node_modules/.bin/babel src --out-dir build",
    "lint": "eslint src/*.js src/**/**.js src/**/**/**.js",
    "test": "./node_modules/.bin/mocha --require @babel/polyfill  --require @babel/register"
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint src/*.js && pretty-quick --staged"
    }
  },
  "repository": "https://github.com/emmsdan/isecure-send-api",
  "author": "EmmsDan",
  "license": "MIT",
  "dependencies": {
    "@babel/node": "^7.6.2",
    "@babel/polyfill": "^7.6.0",
    "adm-zip": "^0.4.13",
    "body-parser": "^1.19.0",
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/register": "^7.6.2",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.6-alpha.5",
    "express-validator": "^6.2.0",
    "mysql": "^2.17.1",
    "supertest": "^4.0.2",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.0.5",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.2",
    "prettier": "1.18.2",
    "pretty-quick": "^1.11.1"
  }
